{
  "name": "fivebeans-icollege",
  "description": "beanstalkd client & worker daemon for node.",
  "version": "1.2.0",
  "author": "C J Silverio <ceejceej@gmail.com>",
  "bin": {
    "beanworker": "./bin/beanworker"
  },
  "bugs": "https://github.com/djtu-association/fivebeans/issues",
  "config": {
    "blanket": {
      "pattern": [
        "lib",
        "index.js"
      ],
      "data-cover-never": [
        "node_modules",
        "test",
        "examples"
      ],
      "output-reporter": "spec",
      "travis-cov": {
        "threshold": 90
      }
    }
  },
  "contributors": [
    "Jon Keating <jon@licq.org>",
    "Jevgenij Tsoi <jevgenij@virtusize.com>",
    "He Li <jet.in.brain@gmail.com>"
  ],
  "dependencies": {
    "js-yaml": "^3.0.1",
    "lodash": "~3.6.0",
    "semver": "~4.3.3",
    "yargs": "~3.6.0"
  },
  "devDependencies": {
    "blanket": "~1.1.6",
    "mocha": "~2.2.1",
    "mocoverage": "~1.0.0",
    "must": "~0.12.0",
    "patched-blanket": "~1.0.1",
    "travis-cov": "~0.2.5"
  },
  "keywords": [
    "beanstalkd",
    "worker",
    "jobs",
    "work-queue"
  ],
  "main": "index",
  "repository": {
    "type": "git",
    "url": "git://github.com/djtu-association/fivebeans.git"
  },
  "scripts": {
    "test": "mocha -t 8000 --check-leaks --ui exports --require patched-blanket -R mocoverage test/test-*.js",
    "test-cov": "mocha -t 20000 --require patched-blanket -R travis-cov test/test-*.js",
    "test-travis": "mocha -R spec -t 20000 test/test-*.js && npm run test-cov"
  }
}
